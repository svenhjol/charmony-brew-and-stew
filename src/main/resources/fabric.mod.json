{
  "schemaVersion": 1,
  "id": "charmony-brew-and-stew",
  "version": "${mod_version}",

  "name": "Charmony Brew and Stew",
  "description": "Adds cooking pots for combining food and casks for combining potions.",

  "authors": [
    "svenhjol",
    "Coranthes"
  ],
  "icon": "assets/charmony/textures/gui/sprites/charmony-brew-and-stew.png",
  "license": "MIT",
  "environment": "*",
  "accessWidener": "charmony-brew-and-stew.accesswidener",
  "entrypoints": {
    "main": [
      "svenhjol.charmony.brew_and_stew.common.CommonInitializer"
    ],
    "client": [
      "svenhjol.charmony.brew_and_stew.client.ClientInitializer"
    ]
  },

  "mixins": [],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "*",
    "java": ">=${java_version}",
    "fabric-api": "*",
    "charmony": ">=${charmony_version}",
    "charmony-api": ">=${api_version}"
  },

  "contact": {
    "homepage": "${homepage_link}",
    "sources": "https://github.com/svenhjol/${mod_id}"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_link}"
      },
      "update_checker": true
    }
  }
}